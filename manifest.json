{
  "manifest_version": 2,
  "name": "Allociné better search",
  "description": "This extension allow add a better search into allociné.com",
  "version": "1.0",
  "browser_action": {
    "default_popup": "dire_bonjour.html",
    "default_icon": "img/allocine_logo.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.allocine.fr/seance/*"
      ],
      "js": [
        "alcBetterSearch.js"
      ]
    }
  ]
}
